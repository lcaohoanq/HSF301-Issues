				<Label layoutX="27.0" layoutY="170.0" text="Password">
					<font>
						<Font size="14.0" />
					</font>
				</Label>
				<TextField id="txtPassword" fx:id="txtPassword"
					layoutX="124.0" layoutY="165.0" prefHeight="26.0" prefWidth="235.0" />
